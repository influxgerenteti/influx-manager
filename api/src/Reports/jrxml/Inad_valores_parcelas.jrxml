	<queryString language="SQL">
		<![CDATA[select
	numero_parcela_documento,
	observacao,
	valor_original,
	date_format(data_prorrogacao,"%d/%m/%Y")  as data_vencimento,
	TIMESTAMPDIFF(DAY,data_prorrogacao,now()) as atraso,
